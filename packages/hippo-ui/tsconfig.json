{
  "compilerOptions": {
      "declaration": true, /* 生成相关的 '.d.ts' 文件。 */
      "declarationDir": "./dist/types", /* '.d.ts' 文件输出目录 */
      "jsx": "preserve",
      "target": "ES2016"
  },
  "include": [
      "./**/*.*",
      "src/shims-vue.d.ts",
  ],
  "exclude": [
      "node_modules",
      "src/.vitepress/**/*"
  ],
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": "true"
}
